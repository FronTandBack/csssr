							div 
								input(type="checkbox" id="check7" checked)
								label(for="check7") ES2015/ES2016
						fieldset 
							div 
								input(type="checkbox" id="check8" checked)
								label(for="check8") Gulp/GRUNT
							div 
								input(type="checkbox" id="check9")
								label(for="check9") Webpack
							div 
								input(type="checkbox" id="check10")
								label(for="check10") Дружу с WebGL
							div 
								input(type="checkbox" id="check11" checked)
								label(for="check11") jQuery
							div 
								input(type="checkbox" id="check12" checked)
								label(for="check12") Изучаю Angular
							div 
								input(type="checkbox" id="check13" checked)
								label(for="check13") Изучаю React
							div 
								input(type="checkbox" id="check14")
								label(for="check14") Знаю/изучаю Node.js
						fieldset
							div 
								input(type="checkbox" id="check15")
